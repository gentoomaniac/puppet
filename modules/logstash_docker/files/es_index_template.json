{
    "template": "coredns-*",
    "settings": {
        "number_of_shards": 1,
        "number_of_replicas": 0,
        "index": {
            "query": {
                "default_field": "@words"
            },
            "store": {
                "compress": {
                    "stored": true,
                    "tv": true
                }
            }
        }
    },
    "mappings": {
        "_default_": {
            "_all": {
                "enabled": false
            },
            "_source": {
                "compress": true
            },
            "dynamic_templates": [
                {
                    "string_template": {
                        "match": "*",
                        "mapping": {
                            "type": "string",
                            "index": "not_analyzed"
                        },
                        "match_mapping_type": "string"
                    }
                }
            ],
            "properties": {
                "@fields": {
                    "type": "object",
                    "dynamic": true,
                    "path": "full"
                },
                "@words": {
                    "type": "string",
                    "index": "analyzed"
                },
                "@source": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "@source_host": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "@source_path": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "@tags": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "@timestamp": {
                    "type": "date",
                    "index": "not_analyzed"
                },
                "@type": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "loglevel": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "clientip": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "type": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "dnsname": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "protocoll": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "stuff": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "status": {
                    "type": "string",
                    "index": "not_analyzed"
                }
            }
        }
    }
}